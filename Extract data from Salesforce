SELECT 
  id contract_id, 
  account_id, 
  start_date, 
  contract_end_date_c end_date, 
  status, 
  payment_type_c, 
  contract_number, 
  contract_no_c,
  date(datetime(created_date, 'Asia/Bangkok')) as contract_created_date,
  time(datetime(created_date, 'Asia/Bangkok')) as contract_created_time,
  file_checker_c,
  rc_tire_c,
  rc_spare_key_c,
  rc_replacement_description_c,
  rc_tire_responsibility_c,
  rc_replacement_within_hr_c,
  deposit_amount_c,
  severity_level_c,
  driver_name_c,
  deductible_amount_c,
  deposit_amount_c,
  fleet_c
FROM `evme-ptt.evme_staging.stg_salesforce_contract`;
